<Defs>
  <!-- Coffee.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Syrcoffee</defName>
    <label>extract syrcoffee seeds</label>
    <description>Extract seeds from SyrCoffee.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SyrRawCoffee</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SyrRawCoffee</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Syrcoffee>3</Seed_Syrcoffee>
    </products>
  </RecipeDef>

  <!-- Tea.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Syrtea</defName>
    <label>extract syrtea seeds</label>
    <description>Extract seeds from SyrTea.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SyrRawTea</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SyrRawTea</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Syrtea>3</Seed_Syrtea>
    </products>
  </RecipeDef>

</Defs>
