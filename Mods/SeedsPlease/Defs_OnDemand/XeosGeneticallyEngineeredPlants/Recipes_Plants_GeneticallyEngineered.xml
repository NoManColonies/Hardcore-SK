<Defs>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedArtiveg</defName>
    <label>extract artiveg seeds</label>
    <description>Extract seeds from Artiveg.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawArtiveg</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawArtiveg</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedArtiveg>2</SeedArtiveg>
    </products>
    <researchPrerequisite>Artiveg</researchPrerequisite>
  </RecipeDef>

